    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "electron": "^25.3.2",
    "electron-builder": "^24.6.3",
    "electron-is-dev": "^2.0.0",
    "vite": "^4.4.5",
    "wait-on": "^7.0.1"
  }
}
