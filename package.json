{
  "name": "cosmic_cube",
  "description": "3D模型压缩和渲染工具",
  "author": "gaoYaTing",
  "private": true,
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "dev": "vite --mode dev",
    "build": "vite build",
